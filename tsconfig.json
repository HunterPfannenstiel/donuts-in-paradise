{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@ui/*": ["components/ui/*"],
      "@constants/*": ["constants/*"],
      "@_types/*": ["types/*"],
      "@hooks/*": ["components/hooks/*"],
      "@store/*": ["store/*"],
      "@screens/*": ["screens/*"]
    },
  },
  "exclude": ["node_modules"]
}